pipeline {
    agent none
    stages{
        stage('Build'){
            agent{
                docker{
                    image 'python:3.8'
                }
            }
            steps{
                withEnv(["HOME=${env.WORKSPACE}"}){
                    sh 'python -m pip install -r django'
                    sh 'python -m py_compile manage.py
                }
            }
        }
    }
}
